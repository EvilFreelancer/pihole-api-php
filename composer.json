{
    "name": "evilfreelancer/pihole-api-php",
    "description": "Pi-Hole API PHP client",
    "homepage": "https://pi-hole.net",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "pi-hole",
        "client",
        "rest-api",
        "api-client"
    ],
    "authors": [
        {
            "name": "Paul Rock",
            "email": "paul@drteam.rocks"
        }
    ],
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "PiHole\\": "./src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PiHole\\Tests\\": "./tests/"
        }
    },
    "require": {
        "php": ">=7.3",
        "ext-curl": "*",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0"
    }
}
